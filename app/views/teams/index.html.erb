<table>
  <tr>
    <th>Match</th>
    <th>Result</th>
  </tr>
  <% for team in @teams %>
    <tr>
      <td><%= team.match_id %></td>
      <td><%= team.result %></td>
      <td><%= link_to "Show", team %></td>
      <td><%= link_to "Edit", edit_team_path(team) %></td>
      <td><%= link_to "Destroy", team, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Team", new_team_path %></p>